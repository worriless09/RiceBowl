# Firebase Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit .env to git!

EXPO_PUBLIC_FIREBASE_API_KEY=your_api_key_here
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your-app.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your-project-id
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your-app.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
EXPO_PUBLIC_FIREBASE_APP_ID=your_app_id

# UPI Payment Configuration
EXPO_PUBLIC_UPI_ID=yourname@upi
EXPO_PUBLIC_UPI_PAYEE_NAME=Your Name
EXPO_PUBLIC_UPI_AMOUNT=299
EXPO_PUBLIC_WHATSAPP_NUMBER=919883536592

# Production Mode (set to 'true' for production release)
EXPO_PUBLIC_IS_PRODUCTION=false
